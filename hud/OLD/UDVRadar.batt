Script "RadarChanger" (Void)
{
	if (GameType() != GAME_TITLE_MAP)
	{
	If( CheckActorProperty(0, APROP_Species, "Marine_Rifle") || CheckActorProperty(0, APROP_NameTag, "EvilMarine_UDV") )
	{
		SetActorProperty(0, APROP_NameTag, StrParam(s:sae_nameslist[random(0,26)][1], s:" ", s:sae_nameslist[random(0,999)][0]));
	}
	Else If (CheckActorProperty(0, APROP_Species, "ZombieMan"))
	{
			SetActorProperty(0, APROP_NameTag, StrParam(s:sae_nameslist[random(0,3)][1], s:" ", s:sae_nameslist[random(0,999)][0]));
	}
	Else If (CheckActorProperty(0, APROP_Species, "ShotgunGuy") || CheckActorProperty(0, APROP_Species, "ZSpecOpsMachinegun") || CheckActorProperty(0, APROP_Species, "Helltrooper") )
	{
		SetActorProperty(0, APROP_NameTag, StrParam(s:sae_nameslist[random(4,14)][1], s:" ", s:sae_nameslist[random(0,999)][0]));
	}
	Else If (CheckActorProperty(0, APROP_Species, "ChaingunGuy"))
	{
		SetActorProperty(0, APROP_NameTag, StrParam(s:sae_nameslist[random(15,26)][1], s:" ", s:sae_nameslist[random(0,999)][0]));
	}
	Else If (CheckActorProperty(0, APROP_Species, "Labguy"))
	{
		SetActorProperty(0, APROP_NameTag, StrParam(s:sae_nameslist[random(27,28)][1], s:" ", s:sae_nameslist[random(0,999)][0]));
	}
	Else If (CheckActorProperty(0, APROP_Species, "Nazi") || CheckActorProperty(0, APROP_Species, "WolfensteinSS") )
	{
		SetActorProperty(0, APROP_NameTag, sae_nameslist[random(29,44)][1]);
	}
		Thing_ChangeTID(ActivatorTID(), tidcounter++);
	}
}

