#define F_RADAR_SIZE 12
str F_RADAR[F_RADAR_SIZE] = 
    {"RADAR1", "RSAFEZ", "RSAFEZ1", "RSAFEZ2", "RSAFEZ3", "RSAFEZ4",
    "RSAFEZ5", "RSAFEZ6", "RSAFEZ7", "RSAFEZ8", "RSAFEZ9", "RSAFEZA"};
script "DrawRadarCircle" (VOID) NET
{
	int atic = 0;
	
	int MRadarC = GetUserCVAR(PlayerNumber(),"UDV_Mradaron");//CheckInventory("MRadarOn");
	
    while(MRadarC == 1) // Escape if gone!
    {
	    int Radar_aspect = GetAspectRatio();
       SetHudSize(640,480,0);
		If(!CheckInventory("sae_extcam") && !CheckInventory("sae_deathcam"))
		{
			SetFont(F_RADAR[atic]);
			If(Radar_aspect == ASPECT_4_3 )
			{ 	
			HudMessage(c:'A'; HUDMSG_PLAIN | HUDMSG_ALPHA, 128, CR_UNTRANSLATED, 560.0, 70.0, 0, 0.25);
			}
			Else
			{
			HudMessage(c:'A'; HUDMSG_PLAIN | HUDMSG_ALPHA, 128, CR_UNTRANSLATED, 650.0, 70.0, 0, 0.25);
			}
		}
		Else //draws nothing so it blanks out the radar
		{
			SetFont("DIGIFONTSMALL");
			{
			HudMessage(c:' '; HUDMSG_PLAIN | HUDMSG_ALPHA, 128, CR_UNTRANSLATED, 560.0, 70.0, 0, 0.25);
			}
		}
		if(atic == 0) // Special case for first/last frames
            delay(8);
        else if(atic == F_RADAR_SIZE - 1)
            delay(10);
        else
            delay(5);
        
        atic++;
        atic %= F_RADAR_SIZE;
	}
}

